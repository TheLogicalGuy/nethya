{
    "name": "NethyaChatBot",
    "description": "Hello, I am Nethya An Intelligent ChatBot. If You Are Feeling Lonely, You can Always Come to me and Chat With Me!",
    "logo": "https://telegra.ph/file/23932e22ece464a1fb06e.jpg",
    "repository": "https://github.com/TheLogicalGuy/nethya",
    "env": {
        "BOT_TOKEN": {
            "description": "telegram bot token here",
            "required": true
        },
        "KUKI_API": {
            "description": "go to @Kukichatbot and then send /token you will get a api paste it here ",
            "required": true
        },
        "ERROR_LOG": {
            "description": "error channel log id",
            "required": true
        },
        "MONGO_URL": {
            "description": "mongo db url here",
            "required": true
        },
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
            "value": ""
        }
        },
        "BOT_USERNAME": {
            "description": "Your Chatbot username without @",
            "required": true
            "value": "nethyabot"
        }
        },
        "SUPPORT_GROUP": {
            "description": "Support group username without @",
            "required": true
             "value": "Awesomesupport"
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
